-- Group: glseries
-- Name:  checkeditable
-- Notes: 
SELECT gltrans_id 
FROM accnt, gltrans 
  LEFT OUTER JOIN period ON (gltrans_date BETWEEN period_start AND period_end) 
WHERE ( (gltrans_accnt_id=accnt_id) 
  AND (gltrans_sequence=<? value("glSequence") ?>) 
  AND ( (gltrans_deleted) 
     OR (period_freeze) 
     OR (gltrans_rec) 
     OR (period_closed AND NOT accnt_closedpost) ) )
LIMIT 1;